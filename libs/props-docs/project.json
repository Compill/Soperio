{
  "root": "libs/props-docs",
  "sourceRoot": "libs/props-docs/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-script",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "script": "build",
        "outputPath": "dist/libs/props-docs",
        "tsConfig": "libs/props-docs/tsconfig.lib.json",
        "project": "libs/props-docs/package.json",
        "entryFile": "libs/props-docs/src/index.ts",
        "assets": [
          {
            "glob": "libs/props-docs/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/props-docs/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/props-docs"],
      "options": {
        "jestConfig": "libs/props-docs/jest.config.js",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  }
}
