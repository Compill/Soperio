{
  "name": "@soperio/cli",
  "version": "1.0.56",
  "description": "A CLI to create custom themes and components",
  "bin": {
    "soperio-cli": "bin/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "clear & rimraf dist & cross-env BABEL_ENV=cjs babel src --root-mode upward --extensions .ts,.tsx -d dist  --source-maps",
    "devSoperio": "clear & node bin/index.js build-theme ./src/lib/test/theme.ts",
    "devSoperioOut": "clear & node bin/index.js build-theme ./src/lib/test/theme.ts  --out ./src/lib/test/out"
  },
  "dependencies": {
    "cli-check-node": "^1.3.4",
    "cli-handle-unhandled": "^1.1.1",
    "cli-welcome": "^2.2.2",
    "commander": "^6.2.1",
    "module-alias": "^2.2.2",
    "ora": "^5.3.0",
    "prettier": "^2.4.1",
    "regenerator-runtime": "^0.13.7",
    "ts-node": "10.4.0",
    "tsconfig-paths": "^3.11.0",
    "update-notifier": "^5.0.1"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.1",
    "@types/ora": "^3.2.0",
    "@types/update-notifier": "5.0.0"
  }
}
