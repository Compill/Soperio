{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cli/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "project": "libs/cli/package.json",
        "entryFile": "libs/cli/src/index.ts",
        "tsConfig": "libs/cli/tsconfig.lib.json",
        "outputPath": "dist/libs/cli"
      }
    },
    "package": {
      "executor": "@nrwl/web:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "project": "libs/cli/package.json",
        "outputPath": "dist/libs/cli",
        "entryFile": "libs/cli/src/index.ts",
        "tsConfig": "libs/cli/tsconfig.lib.json",
        "compiler": "babel",
        "umdName": "OtherName",
        "external": [
          "typescript"
        ],
        "format": [
          "esm"
        ],
        "assets": [
          "libs/cli/src/README.md",
          "libs/cli/src/bin/index.js",
          "libs/cli/src/bin/tsconfig.json"
        ]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs cli {args.ver} {args.tag}"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/cli/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/cli"],
      "options": {
        "jestConfig": "libs/cli/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy",
      "options": {
        "access": "public"
      }
    }
  }
}
